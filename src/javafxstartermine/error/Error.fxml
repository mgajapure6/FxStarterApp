<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxstartermine.error.ErrorController">
   <center>
      <VBox alignment="CENTER" BorderPane.alignment="CENTER">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" text="404 " textFill="#c3c3c3">
               <font>
                  <Font name="System Bold" size="200.0" />
               </font>
               <effect>
                  <InnerShadow color="#00000040" />
               </effect>
            </Label>
            <Label text="Page Not Found" textFill="#c3c3c3">
               <font>
                  <Font name="System Bold" size="40.0" />
               </font>
               <effect>
                  <InnerShadow color="#000000ae" />
               </effect>
            </Label>
         </children>
         <BorderPane.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
      </VBox>
   </center>
</BorderPane>
