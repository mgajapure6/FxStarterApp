<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane minHeight="500.0" minWidth="600.0" stylesheets="@../main.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javafxstartermine.admin.BranchMasterController">
   <center>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
          <Tab closable="false" text="Branch List">
               <content>
                  <VBox>
                     <children>
                        <HBox alignment="CENTER_RIGHT">
                           <children>
                              <Label styleClass="el" text="Search" />
                              <TextField promptText="Search anything" styleClass="el">
                                 <HBox.margin>
                                    <Insets left="10.0" />
                                 </HBox.margin>
                              </TextField>
                           </children>
                           <VBox.margin>
                              <Insets bottom="10.0" top="10.0" />
                           </VBox.margin>
                        </HBox>
                        <TableView prefHeight="200.0" prefWidth="200.0" styleClass="el" VBox.vgrow="ALWAYS">
                          <columns>
                            <TableColumn prefWidth="75.0" text="C1" />
                            <TableColumn prefWidth="75.0" text="C2" />
                          </columns>
                        </TableView>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </VBox>
               </content>
               <styleClass>
                  <String fx:value="el" />
                  <String fx:value="tab-btn" />
               </styleClass>
          </Tab>
          <Tab closable="false" text="Add module">
            <content>
                  <VBox prefHeight="200.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="100.0" prefWidth="200.0" style="-fx-border-color: #ccc; -fx-border-width: 0 0 1 0;">
                           <children>
                              <VBox prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label styleClass="el" stylesheets="@../main.css" text="Branch Code" />
                                    <TextField promptText="Enter Branch Code" styleClass="el" stylesheets="@../main.css" />
                                    <Label styleClass="el" stylesheets="@../main.css" text="Branch Address 1">
                                       <VBox.margin>
                                          <Insets top="15.0" />
                                       </VBox.margin>
                                    </Label>
                                    <TextArea prefHeight="50.0" prefWidth="255.0" promptText="Enter Branch Address" styleClass="el" />
                                    <Label styleClass="el" stylesheets="@../main.css" text="City">
                                       <VBox.margin>
                                          <Insets top="13.0" />
                                       </VBox.margin>
                                    </Label>
                                    <TextField promptText="Enter City / Village" styleClass="el" stylesheets="@../main.css" />
                                    <Label layoutX="30.0" layoutY="30.0" styleClass="el" text="Branch Status">
                                       <VBox.margin>
                                          <Insets top="13.0" />
                                       </VBox.margin>
                                    </Label>
                                    <ComboBox editable="true" prefWidth="15000.0" promptText="Select Branch Status" style="-fx-width: 100%;" styleClass="el" stylesheets="@../main.css" />
                                 </children>
                                 <padding>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </padding>
                              </VBox>
                              <VBox layoutX="10.0" layoutY="10.0" prefHeight="200.0" prefWidth="100.0" HBox.hgrow="ALWAYS">
                                 <children>
                                    <Label styleClass="el" stylesheets="@../main.css" text="Branch Name" />
                                    <TextField promptText="Enter Branch Name" styleClass="el" stylesheets="@../main.css" />
                                    <Label styleClass="el" stylesheets="@../main.css" text="Branch Address 1">
                                       <VBox.margin>
                                          <Insets top="15.0" />
                                       </VBox.margin>
                                    </Label>
                                    <TextArea prefHeight="50.0" prefWidth="255.0" promptText="Enter Branch Address" styleClass="el" />
                                    <Label styleClass="el" stylesheets="@../main.css" text="PIN Code">
                                       <VBox.margin>
                                          <Insets top="15.0" />
                                       </VBox.margin>
                                    </Label>
                                    <TextField promptText="Enter Pin Code" styleClass="el" stylesheets="@../main.css" />
                                    <Label styleClass="el" stylesheets="@../main.css" text="Branch State">
                                       <VBox.margin>
                                          <Insets top="15.0" />
                                       </VBox.margin>
                                    </Label>
                                    <ComboBox editable="true" prefWidth="15000.0" promptText="Select State" style="-fx-width: 100%;" styleClass="el" stylesheets="@../main.css" />
                                    <HBox spacing="10.0">
                                       <VBox.margin>
                                          <Insets top="15.0" />
                                       </VBox.margin>
                                       <children>
                                          <Button mnemonicParsing="false" styleClass="el" stylesheets="@../main.css" text="Clear" />
                                          <Button layoutX="10.0" layoutY="28.0" mnemonicParsing="false" styleClass="el" stylesheets="@../main.css" text="Save Branch" />
                                       </children>
                                       <padding>
                                          <Insets top="18.0" />
                                       </padding>
                                    </HBox>
                                 </children>
                                 <padding>
                                    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                                 </padding>
                              </VBox>
                           </children>
                        </HBox>
                     </children>
                  </VBox>
            </content>
               <styleClass>
                  <String fx:value="el" />
                  <String fx:value="tab-btn" />
               </styleClass>
          </Tab>
        </tabs>
      </TabPane>
   </center>
</BorderPane>
